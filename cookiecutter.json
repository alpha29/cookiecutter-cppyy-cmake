{
    "full_name": "Your name",
    "email": "Your address email (eq. you@example.com)",
    "github_username": "Your github username",
    "repo_name": "Name of the project repo",
    "cmake_project_name": "{{ cookiecutter.repo_name.lower().replace(' ', '_').replace('-', '_') }}",
    "cpp_lib_name": "Name of the c++ project",
    "pkg_name": "Python package name for the bindings",
    "project_short_description": "cppyy-generated bindings for {{ cookiecutter.cpp_lib_name }}",
    "project_url": "https://github.com/{{ cookiecutter.github_username }}/{{ cookiecutter.repo_name }}",
    "release_date": "{% now 'local' %}",
    "version": "0.1.0",
    "cpp_version": "14",
    "_extensions": ["jinja2_time.TimeExtension"]
}
